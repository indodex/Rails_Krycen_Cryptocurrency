#sign-in-sign-up
  #sign-in-sign-up-tabs.tabs.tabs-top.tab-container
    ul.etabs style="text-align: center;"
      li.tab
        a href="#sign-up" Register
      li.tab
        a href="#sign-in" Login
    .panel-container
      #sign-in.tab-block
        = render partial: 'shared/sign_in', locals: { identity: Identity.new, require_captcha: false }
      #sign-up.tab-block
        = render partial: 'shared/sign_up', locals: { identity: Identity.new }
  #forget_pwd_form
    = render partial: 'shared/forget_password', locals: { token: Token::ResetPassword.new }