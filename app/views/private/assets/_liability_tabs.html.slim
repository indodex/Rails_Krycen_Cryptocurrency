.tabbable
  ul.nav.nav-tabs
    li.active: a.btc-proof href="#btc-proof" data-toggle="tab" == t('.verify-btc')
    /li:        a.cny-proof href="#cny-proof" data-toggle="tab" == t('.verify-cny')
    li:        a.ltc-proof href="#ltc-proof" data-toggle="tab" == t('.verify-ltc')
    li:        a.oct-proof href="#oct-proof" data-toggle="tab" == t('.verify-oct')
    li:        a.doge-proof href="#doge-proof" data-toggle="tab" == t('.verify-doge')
    li:        a.dsh-proof href="#dsh-proof" data-toggle="tab" == t('.verify-dsh')
    /li:        a.xrp-proof href="#xrp-proof" data-toggle="tab" == t('.verify-xrp')
    /li:        a.eth-proof href="#eth-proof" data-toggle="tab" == t('.verify-eth')
    /li:        a.bts-proof href="#bts-proof" data-toggle="tab" == t('.verify-bts')
    /li:        a.usdt-proof href="#usdt-proof" data-toggle="tab" == t('.verify-usdt')
    /li:        a.dash-proof href="#dash-proof" data-toggle="tab" == t('.verify-dash')
    /li:        a.trmb-proof href="#trmb-proof" data-toggle="tab" == t('.verify-trmb')
    /li:        a.xmr-proof href="#xmr-proof" data-toggle="tab" == t('.verify-xmr')
    li:        a.rby-proof href="#rby-proof" data-toggle="tab" == t('.verify-rby')
    /li:        a.ybc-proof href="#ybc-proof" data-toggle="tab" == t('.verify-ybc')
    /li:        a.xpm-proof href="#xpm-proof" data-toggle="tab" == t('.verify-xpm')
    li: a.gxc-proof href="#gxc-proof" data-toggle="tab" == t('.verify-gxc')
    li: a.ggc-proof href="#ggc-proof" data-toggle="tab" == t('.verify-ggc')
    li: a.opr-proof href="#opr-proof" data-toggle="tab" == t('.verify-opr')
  .tab-content
    #btc-proof.tab-pane.active: .trade-wrapper
      .assets
        span.title = t('.btc-assets-total')
        span.currency
          == t("currency.icon.btc")
          = @btc_proof.sum
      = render 'liability_data', account: @btc_account, proof: @btc_proof

